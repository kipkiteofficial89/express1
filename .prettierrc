{
    "singleQuote": false,
    "trailingComma": "es5",
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "printWidth": 80,
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "bracketSameLine": true,
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "ignore",
    "jsxSingleQuote": false,
    "jsxBracketSameLine": false,
    "quoteProps": "as-needed",
    "proseWrap": "preserve",
    "requireConfig": false,
    "embeddedLanguageFormatting": "auto",
    "vueIndentScriptAndStyle": false
}